<main>
  <h1>Browser</h1>
  <form method="get">
    <input type="text" class="search" name="search" placeholder="Search...">
    <input type="number" name="fromLevel" min="1" max="1000" class="difficulty" value="{{fromLevel}}" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="toLevel" min="1" max="1000" class="difficulty" value="{{toLevel}}" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>

  {{#each cubes}}
    {{> cubeCard}}
  {{/each}}
</main>
